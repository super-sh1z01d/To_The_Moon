[Unit]
Description=To The Moon Pump.fun WebSocket Worker
After=network.target

[Service]
Type=simple
User=www-data
Group=www-data
WorkingDirectory=/srv/tothemoon
EnvironmentFile=/etc/tothemoon.env
ExecStart=/usr/bin/python3 -m src.workers.pumpfun_ws
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target

